        max                        INT       2
        default                    INT       2
        maya.channelbox     BOOL    True
    [attr divideColorByOpacity]
        desc                        STRING "Divide"
        min                         INT       0
        max                        INT       2
        default                    INT       1
        maya.channelbox     BOOL    True
    [attr useMaximumRayLength]
        desc                       STRING "Enable maximum ray length"
        default                    BOOL   False
        maya.channelbox     BOOL    True
