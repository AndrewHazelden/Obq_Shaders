        max                        FLOAT   1
        default                    FLOAT  0.5
        maya.channelbox     BOOL    True
    [attr useBiasGainDot]
        desc                        STRING "Enable"
        default                    BOOL   False
        maya.channelbox     BOOL    True
    [attr biasDot]
        desc                        STRING "Bias"
        min                         FLOAT   0
        max                        FLOAT   1
        default                    FLOAT  0.7
        maya.channelbox     BOOL    True
    [attr gainDot]
        desc                       STRING "Gain"
        min                        FLOAT   0
        max                        FLOAT   1
        default                    FLOAT  0.9
        maya.channelbox     BOOL    True
    [attr radiusFalloffMode]
        desc                       STRING "Mode"
        min                        FLOAT    0
        max                        FLOAT   2
        default                    FLOAT   0
        maya.channelbox     BOOL    True
    [attr falloffStart]
